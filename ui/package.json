{
  "name": "adw-documentation-system",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "ng serve --open --proxy-config src/proxy.conf.js --host 0.0.0.0",
    "test": "ng test --watch",
    "test:ci": "ng test --watch=false --browsers ChromeHeadless",
    "test:coverage": "ng test --watch=true --code-coverage=true",
    "test:coverage:ci": "ng test --watch=false --code-coverage=true --browsers ChromeHeadless",
    "build:dev": "ng build --progress --output-path ../public",
    "build:prod": "ng build --progress --prod --i18n-file src/locale/messages.de.xlf --i18n-format xlf --i18n-locale de --output-path ../public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@types/chart.js": "^2.9.13",
    "angular-tree-component": "^8.5.2",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "chartjs-plugin-datalabels": "^0.7.0",
    "core-js": "^3.6.4",
    "ionicons": "^4.6.3",
    "jquery": "^3.4.1",
    "keycloak-angular": "^7.0.0",
    "keycloak-js": "^8.0.2",
    "moment": "^2.24.0",
    "ngx-bootstrap": "^5.3.2",
    "ngx-md": "^6.0.9",
    "ngx-toastr": "^10.1.0",
    "popper": "^1.0.1",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.4",
    "uuid": "^3.4.0",
    "zone.js": "^0.10.2"
  },
